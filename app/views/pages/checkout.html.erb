<div class="test">
<h1>Check Out</h1>
</div>


<% if flash[:notice] %>
  <div class="notice"><%= flash[:notice] %></div>
<% end %>

<div class="form">
     <tbody>
         <%= form_for(@rental) do |f| %>
         <p>
              <%= f.label :UIN %><br>
               <%= f.text_field :UIN , :maxlength => 9%>
         </p>
         <p>
             <%= f.label :Rental_ID %><br>
               <%= f.text_field :Rental_ID %>
         </p>
         <p>
              <%= f.label :Apparel_ID %><br>
               <%= f.text_field :Apparel_ID %>
         </p>
         <p>
             <%= f.label :Checkout_Date %><br>
               <%= f.date_field :Checkout_Date %>
         </p>
         <p>
             <%= f.label :Expected_Return_Date %><br>
               <%= f.date_field :Expected_Return_Date %>
         </p>
         <p>
             <%= f.submit class: "btn-btn-primary3" %>
         </p>
         <% end %>
     </tbody>
 </div> 

